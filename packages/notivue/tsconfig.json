{
   "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020",
      "moduleResolution": "Node",
      "strict": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "lib": ["ESNext", "DOM"],
      "skipLibCheck": true,
      "noEmit": true
   },
   "include": ["."]
}
