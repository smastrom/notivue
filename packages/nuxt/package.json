{
   "name": "@nuxtjs/notivue",
   "version": "1.2.4",
   "private": false,
   "description": "Fully-featured toast notification system for Nuxt",
   "keywords": [
      "vue",
      "vuejs",
      "notification",
      "notifications",
      "alert",
      "alerts",
      "toast",
      "toasts",
      "notivue",
      "nuxt-module",
      "nuxt",
      "nuxt3"
   ],
   "homepage": "https://notivue.pages.dev/",
   "bugs": {
      "url": "https://github.com/smastrom/notivue/issues"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/smastrom/notivue.git",
      "directory": "packages/nuxt"
   },
   "license": "MIT",
   "author": {
      "name": "Simone Mastromattei",
      "email": "smastrom@proton.me"
   },
   "sideEffects": false,
   "exports": {
